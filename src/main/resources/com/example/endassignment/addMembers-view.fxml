<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <DatePicker id="birthdate" fx:id="birthdate" layoutX="380.0" layoutY="127.0" promptText="dd/mm/yyyy"/>
        <TextField id="firstname" fx:id="firstname" layoutX="54.0" layoutY="115.0" promptText="Fisrt Name"/>
        <TextField id="lastname" fx:id="lastname" layoutX="54.0" layoutY="175.0" promptText="Last Name"/>
        <Button id="addMemberButton" fx:id="addMemberButton" layoutX="64.0" layoutY="273.0" mnemonicParsing="false"
                onAction="#onAddMemberButton" text="Add member"/>
        <Button id="cancelButtonAdd" fx:id="cancelButtonAdd" layoutX="203.0" layoutY="273.0" mnemonicParsing="false"
                onAction="#onCancelButton" text="Cancel"/>

        <Label id="displayErrorMessageMember" fx:id="displayErrorMessageMember" layoutX="40.0" layoutY="40.0" text=""/>
        <Label layoutX="54.0" layoutY="58.0" text="Add Member:">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
