        <TextField id="author" fx:id="author" layoutX="54.0" layoutY="175.0" promptText="Enter Author" />
        <Button id="addItemButton" fx:id="addItemButton" layoutX="64.0" layoutY="273.0" mnemonicParsing="false" onAction="#onAddItemButton" text="Add Item" />
        <Button id="cancelButtonAddItem" fx:id="cancelButtonAddItem" layoutX="203.0" layoutY="273.0" mnemonicParsing="false" onAction="#onCancelButton" text="Cancel" />
        <Label id="displayUsefulMessage" fx:id="displayUsefulMessage" layoutX="40.0" layoutY="40.0" text="" />
        <Label layoutX="54.0" layoutY="58.0" text="Add Item:">
            <font>
